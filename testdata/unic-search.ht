{
    Name: "Unic: Search",           // Basicaly the ID of this Test
    Description: "",                // What is this Test good for?
    Request: {                      // The details of the HTTP request
        URL: "https://{{HOST}}/de/tools/suche.html",    // URL is the URL (it may contain query parameters)
        Params: {                   // Params contains the query parameters
            z: 57,                  // will produce z=57
            y: "foo",               // will produce y=foo
            q: "{{QUERY}}",         // the variable QUERY (see below) is substituted, produces q=design on first run
            w: [ "A B", "über" ],   // several values are possible, all properly escaped w=A%20B&w=%C3%BCber
	    a: "{{RANDOM NUMBER 300 %05d}}",
        }
    },
    Timeout: "6s",     // Abort if not resonable fast
    Checks: [
        { Check: "StatusCode", Expect: 200 },                   // Check status code is OK
        { Check: "ResponseTime", Lower: "1.5s",  Higher: 0 },   // Check time-to-last-byte < 1.5 seconds
        { Check: "Body", Contains: "© 2015 Unic", Count: 1 },   // Body should contain exactly one copyright notice
	{ Check: "Body", Regexp: "foo.e+" },                    
    ],
    Unroll: {
        // This is one test, but repeated 3 times with different values substituted 
	// for the variable {{QUERY}}: What you see here is unrolled 3 times.
        QUERY: [ "design", "hybris", "kiesabbau" ],
    },
    Verbosity: 2,
    Criticality: "Info",
}
