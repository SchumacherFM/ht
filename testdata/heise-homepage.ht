{
    Name: "Heise Homepage",
    Description: "The homepage of heise",
    BasedOn: [
        "german-chrome.mixin",
        "default-webpage.mixin"
    ],
    Request: { URL: "http://www.heise.de/" },
    Checks: [    // These checks are added to checks brought by the mixins.
        {Check: "StatusCode", Expect: 200},
        {Check: "Header", Header: "X-Clacks-Overhead", Equals: "GNU Terry Pratchett"},
	{Check: "HTMLTag", Selector: "div#logo_bereich div.logo", Count: 1},
	{Check: "HTMLContains", Selector: "head title",
          Text: ["heise online - IT-News, Nachrichten und Hintergr√ºnde"]},
    ],
    Verbosity: 1
}
