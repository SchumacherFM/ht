{
    Name: "Complex"
    Description: "Complex Variable Replacement in Body"

    Method: "POST"
    URL: "https://localhost:8880/{LANG}/greet"

    ParseForm: true
    Map: [
        {
            Variables: [ "last",       "title"   ],
            Table:     [ "Dumbledore", "Prof. ",
                         "Jones",      "Dr. ",
                         "*",          ""        ]
        }
        {
            Variables: [ "last",       "middle"     ],
            Table:     [ "Jones",      "'Indiana' ",
                         "*",          ""           ]
        }
    ]

    Response: {
        StatusCode: 200
        Header: {
            "X-Custom-Header": "trivial"
        }
        Body: "@vfile:greet_{{LANG}}.txt"
    }

    Variables: {
        title: ""
        middle: ""
    }
}